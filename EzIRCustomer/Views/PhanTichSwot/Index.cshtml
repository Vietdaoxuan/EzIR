                                        }

                                        if (value.menuID == SWOT_CONFIGS.MENUID_CHALLENGE) {
                                            $('#txtMenuID_Challenge').val(value.menuID);
                                            $('#txtID_Challenge').val(value.id);
                                            $('#txtApprove_Challenge').val(value.approve);

                                            if (!CKEDITOR.instances.editorChallenge) CreateEditor();

                                            CKEDITOR.instances.editorChallenge.setData(value.content);

                                            if (value.approve == 1 || value.approve == 2) CKEDITOR.instances.editorChallenge.setReadOnly(true);
                                        }
                                    }
                                }
                            }

                        })
                        .fail((err) => {
                            $('#loading').removeClass('loading');
                            //console.log(err);
                        });
                    }, 500);


                }

                var registerEvents = function () {

                    // Save SOWT
                    $('body').on('click', '#btnSave', function () {

                        $('#loading').addClass('loading');

                        // status key press
                        let keyPress_Strength = $('#keyPress_Strength').val();
                        let keyPress_Weakness = $('#keyPress_Weakness').val();
                        let keyPress_Opportunity = $('#keyPress_Opportunity').val();
                        let keyPress_Challenge = $('#keyPress_Challenge').val();

                        let infoSheetViewModels = [];

                        if (keyPress_Strength == SWOT_CONFIGS.IS_KEY_PRESS) {
                            let dataEditorStrength = CKEDITOR.instances.editorStrength.getData();
                            let idStrength = $('#txtID_Strength').val();
                            let menuIdStrength = $('#txtMenuID_Strength').val();
                            //let approveStrength = $('#txtApprove_Strength').val();
