                                    type: 'success',
                                    confirmButtonText: 'OK'
                                });
                                //toastr.success(data.message);
                                return;
                            } else {
                                return toastr.error(data.message);
                            }

                        })
                        .fail((err) => {
                            //console.log(err);
                            $('#loading').removeClass('loading');
                        });

