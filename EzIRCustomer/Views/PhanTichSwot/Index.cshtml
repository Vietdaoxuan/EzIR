            // Object Swot
            var Swot = function () {

                this.init = function () {
                    CreateEditor();
                    setTimeout(function () {
                        registerEvents();
                        loadData();
                    }, 500);
                }

                var loadData = async function () {

                    $('#loading').addClass('loading');

                    destroyCKEDITOR();

                    setTimeout(function () {
                        $.ajax({
                        url: '@Url.Action("GetSwot", "PhanTichSWOT")',
                        method: 'GET',
                        dataType: 'json',
                        data: {  }
                        })
                        .done((data) => {

                            //console.log(data);

                            $('#loading').removeClass('loading');

                            if (data.code == 0) {

                                if (Array.isArray(data.data) && data.data.length > 0) {
                                    let arrObj = data.data;

                                    for (const value of arrObj) {

                                        if (value.menuID == SWOT_CONFIGS.MENUID_STRENGTH) {
                                            $('#txtMenuID_Strength').val(value.menuID);
                                            $('#txtID_Strength').val(value.id);
                                            $('#txtApprove_Strength').val(value.approve);

                                            if (!CKEDITOR.instances.editorStrength) CreateEditor();

                                            CKEDITOR.instances.editorStrength.setData(value.content);

                                            if (value.approve == 1 || value.approve == 2) CKEDITOR.instances.editorStrength.setReadOnly(true);
                                        }

                                        if (value.menuID == SWOT_CONFIGS.MENUID_WEAKNESS) {
                                            $('#txtMenuID_Weakness').val(value.menuID);
                                            $('#txtID_Weakness').val(value.id);
                                            $('#txtApprove_Weakness').val(value.approve);

