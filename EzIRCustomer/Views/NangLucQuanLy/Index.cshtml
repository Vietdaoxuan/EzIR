            // Object NLQL
            var NLQL = function () {

                this.init = function () {
                    CreateEditor();
                    setTimeout(function () {
                        registerEvents();
                        loadData();
                    }, 500);
                }

                var loadData = async function () {

                    $('#loading').addClass('loading');

                    destroyCKEDITOR();

                    setTimeout(function () {
                        $.ajax({
                        url: '@Url.Action("GetNlql", "NangLucQuanLy")',
                        method: 'GET',
                        dataType: 'json',
                        data: {  }
                        })
                        .done((data) => {

                            //console.log(data);

                            $('#loading').removeClass('loading');

                            if (data.code == 0) {

                                if (Array.isArray(data.data) && data.data.length > 0) {
                                    let arrObj = data.data;

                                    for (const value of arrObj) {

                                        if (value.menuID == NLQL_CONFIGS.MENUID_NLQLCL) {
                                            $('#txtMenuID_NLQLCL').val(value.menuID);
                                            $('#txtID_NLQLCL').val(value.id);
                                            $('#txtApprove_NLQLCL').val(value.approve);

                                            if (!CKEDITOR.instances.editorNLQLCL) CreateEditor();

                                            CKEDITOR.instances.editorNLQLCL.setData(value.content);

                                            if (value.approve == 1 || value.approve == 2) CKEDITOR.instances.editorNLQLCL.setReadOnly(true);
                                        }

                                        if (value.menuID == NLQL_CONFIGS.MENUID_NLQLTC) {
                                            $('#txtMenuID_NLQLTC').val(value.menuID);
                                            $('#txtID_NLQLTC').val(value.id);
                                            $('#txtApprove_NLQLTC').val(value.approve);

