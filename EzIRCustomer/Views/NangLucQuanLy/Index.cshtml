                                            $('#txtMenuID_NLQLK').val(value.menuID);
                                            $('#txtID_NLQLK').val(value.id);
                                            $('#txtApprove_NLQLK').val(value.approve);

                                            if (!CKEDITOR.instances.editorNLQLK) CreateEditor();

                                             CKEDITOR.instances.editorNLQLK.setData(value.content);

