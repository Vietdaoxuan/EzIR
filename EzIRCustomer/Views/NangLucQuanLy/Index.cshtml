                        let keyPress_NLQLTC = $('#keyPress_NLQLTC').val();
                        let keyPress_NLQLNS = $('#keyPress_NLQLNS').val();
                        let keyPress_NLQLK = $('#keyPress_NLQLK').val();

                        let infoSheetViewModels = [];

                        if (keyPress_NLQLCL == NLQL_CONFIGS.IS_KEY_PRESS) {
                            let dataEditorNLQLCL = CKEDITOR.instances.editorNLQLCL.getData();
                            let idNLQLCL = $('#txtID_NLQLCL').val();
                            let menuIdNLQLCL = $('#txtMenuID_NLQLCL').val();
                            //let approveNLQLCL = $('#txtApprove_NLQLCL').val();
